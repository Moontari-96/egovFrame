<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="egovframework.example.admin.menu.service.impl.AdminMenuMapper">
	<!-- 관리자 메뉴 -->
	<select id="selectMenuList" resultType="AdminMenuVO">
	   	select 
            *
        from 
        	menu
        order by 
        	parent_id, menu_id
	</select>
</mapper>